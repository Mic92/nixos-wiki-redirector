{
  "manifest_version": 2,
  "name": "NixOS Wiki Redirect",
  "version": "1.0",
  "description": "Redirects nixos.wiki to wiki.nixos.org",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://nixos.wiki/*"
  ],
  "applications": {
    "gecko": {
      "id": "joerg@thalheim.io",
      "strict_min_version": "1.2.3"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
